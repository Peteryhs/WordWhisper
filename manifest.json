{
    "name":"Autocompleter",
    "version":"1.0",
    "description":"Autocompleter for chrome",
    "manifest_version":3,
    "author":"Fatass and Terrorist",
    "permissions": ["storage","notifications","tabs"],
    "action":{
        "default_popup":"index.html",
        "default_width": 500,
        "default_height": 500},
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
    }
    ]
}